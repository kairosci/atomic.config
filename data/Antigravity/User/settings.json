{
    "git.autofetch": true,
    "git.confirmSync": false,
    "search.searchView.keywordSuggestions": true,
    "editor.suggest.showWords": false,
    "editor.foldingMaximumRegions": 9999,
    "editor.showFoldingControls": "always",
    "notebook.showFoldingControls": "always",
    "editor.foldingImportsByDefault": true,
    "json.maxItemsComputed": 9999,
    "xml.symbols.maxItemsComputed": 9999,
    "yaml.maxItemsComputed": 9999,
    "files.autoSave": "onFocusChange",
    "files.autoSaveDelay": 9999,
    "files.autoSaveWhenNoErrors": true,
    "editor.codeActions.triggerOnFocusChange": true,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 99,
    "editor.wordBasedSuggestions": "off",
    "notebook.output.wordWrap": true,
    "editor.wrapOnEscapedLineFeeds": true,
    "typescript.preferences.preferTypeOnlyAutoImports": true,
    "editor.fontFamily": "Adwaita Mono, 'monospace', monospace",
    "workbench.settings.applyToAllProfiles": [],
    "zig.zls.enabled": "on"
}